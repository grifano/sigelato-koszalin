{
    "headers": [
      {
        "source": "/video/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "GET, OPTIONS"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "Content-Type"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/video/sigelato-movie.mp4",
        "destination": "/api/cors"
      }
    ]
  }